{
  "name": "fastify-prisma",
  "version": "1.0.0",
  "description": "Basic implementation of Prisma with Fastify",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "db:save": "prisma migrate dev --create-only --preview-feature",
    "db:migrate": "prisma migrate dev --preview-feature",
    "db:seed": "prisma db seed --preview-feature"
  },
  "keywords": [],
  "author": "Ryan",
  "license": "ISC",
  "dependencies": {
    "@prisma/cli": "2.15.0",
    "@prisma/client": "2.15.0",
    "fastify": "3.10.1",
    "fastify-cors": "^5.1.0",
    "fastify-helmet": "^5.1.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.22",
    "@types/prettier": "^2.1.6",
    "husky": ">=4",
    "lint-staged": ">=10",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "ts-eager": "^1.1.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "typesync": "^0.8.0"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "arrowParens": "avoid"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js,json,md}": "prettier --write"
  }
}
